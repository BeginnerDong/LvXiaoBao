.paid{
	width: 90%;
	height: 400px;
	box-shadow:0px 2px 3px 0px rgba(222,222,222,1);
	background: #F0F0F0;
	margin-left: 5%;
	margin-right: 3%;
}
.pid{
	width: 100%;
	height: 250px;
}
.paid-img{
	width: 100%;
	height: 100px;
	text-align: center;
	padding-top: 5%;
}
.paid-img image{
	width: 100px;
	height: 100px;
	border: 1px dashed #323232;
}
.paid-title{
	width: 100%;
	height: 30px;
	text-align: center;
	color: #323232;
	font-size: 17pt;
	margin-top: 2%;
}
.paid-price{
	width: 100%;
	height: 30px;
	text-align: center;
	color: #EC6532;
	font-size: 14pt;
}
.paid-name{
	width: 100%;
	height: 40px;
	color: #323232;
	font-size: 16pt;
	padding-left: 3%;
}
.paid-con{
	width: 100%;
	height: 50px;
	display: flex;
	padding-left: 3%;
}
.con{
	width: 30%;
	height: 50px;
	color: #585858;
	font-size: 15px;
}
.mou{
	width: 100%;
	height: 40px;
	display: flex;
}
.mou-title{
	width: 50%;
	height: 40px;
	padding-left: 3%;
	color: #A1A1A1;
	font-size: 13px;
	color: #A1A1A1;
	opacity: 0.6;
}
.paid-btn{
	width: 90%;
	height: 40px;
	background: #FF582B;
	color: white;
	text-align: center;
	line-height: 40px;
	border-radius: 30px;
	margin-left: 3%;
	margin-top: 7%;
}
.pid1{
	width: 100%;
	height: 250px;
}
.paid-price1{
	width: 100%;
	height: 30px;
	text-align: center;
	font-size: 14pt;
}
.payment1{
	width: 100%;
	height: 50px;
	display: flex;
}
.view{
	width: 40%;
	height: 40px;
	border-radius: 30px;
	border: 1px solid #585858;
	color: #585858;
	opacity: 0.6;
	margin-left: 6%;
	line-height: 40px;
	text-align: center;
}
.home{
	width: 40%;
	height: 40px;
	border-radius: 30px;
	border: 1px solid #EC6532;
	color: #EC6532;
	opacity: 0.6;
	margin-left: 4%;
	line-height: 40px;
	text-align: center;
}