page{
	background: #F4F3F3;
}
.paid{
	width: 90%;
	height: 400px;
	box-shadow:0px 2px 3px 0px rgba(222,222,222,1);
	background: white;
	margin-left: 5%;
	margin-right: 3%;
	margin-top: 20px;
}
.pid{
	width: 100%;
	height: 250px;
}
.paid-img{
	width: 100%;
	height: 100px;
	text-align: center;
	padding-top: 5%;
}
.paid-img image{
	width: 100px;
	height: 100px;
	border: 1px dashed #323232;
}
.paid-title{
	width: 100%;
	height: 30px;
	text-align: center;
	color: #323232;
	font-size: 16px;
	margin-top: 3%;
	font-weight: 500;
}
.paid-price{
	width: 100%;
	height: 30px;
	text-align: center;
	color: #EC6532;
	font-size: 14px;
	font-weight: bold;
}
.paid-name{
	width: 100%;
	height: 40px;
	color: #323232;
	font-size: 16px;
	padding-left: 3%;
}
.paid-con{
	width: 100%;
	height: 50px;
	display: flex;
	padding-left: 3%;
}
.con{
	width: 30%;
	height: 50px;
	color: #585858;
	font-size: 15px;
}
.mou{
	width: 100%;
	height: 40px;
	display: flex;
}
.mou-title{
	width: 50%;
	height: 40px;
	padding-left: 3%;
	color: #A1A1A1;
	font-size: 13px;
	color: #A1A1A1;
	opacity: 0.6;
}
.paid-btn{
	width: 90%;
	height: 40px;
	background: #FF582B;
	color: white;
	text-align: center;
	line-height: 40px;
	border-radius: 30px;
	margin-left: 3%;
	margin-top: 7%;
}
.pid1{
	width: 100%;
	height: 250px;
}
.paid-price1{
	width: 100%;
	height: 30px;
	text-align: center;
	font-size: 13px;
	font-weight: 500;
	color: #A1A1A1;
}
.payment1{
	width: 100%;
	height: 200px;
	text-align: center;
}
.view{
	width: 100%;
	height: 60px;
	text-align: center;
}
.view span{
	width: 200px;
	height: 45px;
	border-radius: 30px;
	color: white;
	background: #FF582B;
	margin-left: 6%;
	line-height: 40px;
	text-align: center;
	display: inline-block;
}
.home{
	width: 100%;
	height: 60px;
	text-align: center;
}
.home span{
	width: 200px;
	height: 45px;
	border-radius: 30px;
	color:#A1A1A1;
	background: #FFFFFF;
	margin-left: 6%;
	line-height: 40px;
	text-align: center;
	display: inline-block;
}