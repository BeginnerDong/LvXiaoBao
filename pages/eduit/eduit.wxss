.eduit{
	width: 100%;
	/* height: 800rpx; */
}
.peo-name{
	width: 100%;
	height: 100rpx;
	display: flex;
	padding: 0 3%;
  box-sizing: border-box;
	border-bottom: 2rpx solid #E7E7E7;
}
.peo-con{
	width: 30%;
	height: 100rpx;
	font-size: 32rpx;
	color: rgba(50,50,50,1);
	font-weight: 500;
	line-height: 100rpx;
}
.peo-content{
	width: 70%;
	height: 100rpx;
	line-height: 100rpx;
	display: flex;
}
.peo-content input{
	width: 100%;
	height: 96rpx;
	border: none;
	line-height: 96rpx;
	font-size: 24rpx;
	color: #A1A1A1;
}
.card-img{
	width: 20%;
	height:100rpx;
	text-align:center;
}
.card{
	width: 100%;
	
}
.card input{
	width: 100%;
	height: 96rpx;
	border: none;
  font-size: 28rpx;
	color: #A1A1A1;
	line-height: 96rpx;
	
}
.card-img image{
	width: 30rpx;
	height: 30rpx;
}

.peo-content1{
	width: 70%;
	height: 100rpx;
	line-height: 100rpx;
	font-size: 28rpx;
	color: #A1A1A1;
}
.peo-content1 input{
	width: 69%;
	height: 96rpx;
	border: none;
	line-height: 96rpx;
	font-size: 28rpx;
	color: #A1A1A1;
	opacity: 1;
}
.btn{
	width: 90%;
	height: 100rpx;
	background:linear-gradient(90deg,rgba(255,88,43,1) 0%,rgba(255,123,87,1) 100%);
	color: white;
	text-align: center;
	border-radius: 60rpx;
	line-height: 100rpx;
	margin-left: 5%;
	opacity: 0.3;
	margin-top: 40rpx;
	font-size: 34rpx;
	font-weight: bold;
}