page {
	background: #F4F3F3;
}

.top {
	width: 100%;
	background: #FFD101;
	z-index: 222;
}

.search {
	width: 100%;
	padding: 20rpx 0;
}

.search input {
	width: 85%;
	background: #fff;
	border-radius: 40rpx;
	height: 80rpx;
	padding: 0 20rpx;
	position: relative;
	margin: 0 auto;
	font-size: 28rpx;
	font-family: PingFang-SC-Medium;
	font-weight: 500;
	color: rgba(192, 192, 192, 1);
}

.searchBtn {
	width: 100rpx;
	height: 80rpx;
	background: rgba(50, 50, 50, 1);
	border-radius: 0px 40rpx 40rpx 0px;
	position: absolute;
	right: 4.5%;
	top: 20rpx;
	display: flex;
	align-items: center;
	justify-content: center;
	z-index: 222
}

.searchBtn image {
	width: 40rpx;
	height: 40rpx;
}

.title-line {
	width: 32rpx;
	height: 6rpx;
	background: rgba(255, 209, 1, 1);
	border-radius: 4rpx;
	position:absolute;
	bottom:10rpx;
	z-index:-1;
}

.nav-top {
	width: 100%;
	height: 100rpx;
	background: #fff;
	z-index: 222;
}

.nav-top-item {
	width: 20%;
	text-align: center;
	font-size: 30rpx;
	font-family: PingFang-SC-Medium;
	font-weight: 500;
	color: rgba(50, 50, 50, 1);
	line-height: 50rpx;
	height:50rpx;
	position: relative;
	z-index:222
}
.nav-top-item-active{
	font-weight: 700;
}
.nav-top-item image{
	width:40rpx;
	height:40rpx
}
.peer{
	width:87%;
	margin: 0 auto;
	background: #fff;
	box-shadow:0px 4rpx 6rpx 0px rgba(222,222,222,1);
	border-radius:24rpx;
	height:380rpx;
	margin-top: 24rpx;
	padding: 3%;
}
.peer-top{
	height:120rpx;
	border-bottom: 2rpx solid #E7E7E7;
}
.qian image{
	width:40rpx;
	height:40rpx
}
.phone{
	width:80rpx;
	height:80rpx;
	background:rgba(244,243,243,1);
	border-radius:50%;
	display: flex;
	align-items: center;
	justify-content: center;
}
.phone image{
	width:40rpx;
	height:40rpx;
}
.zhuying{
	width:144rpx;
	height:48rpx;
	background:rgba(255,209,1,1);
	border-radius:24rpx;
	line-height: 48rpx;
	text-align: center;
}
.peer-middle{
	height:160rpx;
	border-bottom: 1px solid #E7E7E7;
}
.peer-bottom{
	width:100%;
	height:120rpx
}
.peer-bottom-item{
	width:33.3%
}
.peer-bottom-item image{
	width:32rpx;
	height:32rpx
}
