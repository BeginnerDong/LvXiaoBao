.search {
	width: 100%;
	padding: 20rpx 0;
}

.search input {
	width: 85%;
	background: #fff;
	border-radius: 40rpx;
	height: 80rpx;
	padding: 0 20rpx;
	position: relative;
	margin: 0 auto;
	font-size: 14px;
	font-family: PingFang-SC-Medium;
	font-weight: 500;
	color: rgba(192, 192, 192, 1);
}

.searchBtn {
	width: 100rpx;
	height: 80rpx;
	background: rgba(50, 50, 50, 1);
	border-radius: 0px 20px 20px 0px;
	position: absolute;
	right: 4.5%;
	top: 20rpx;
	display: flex;
	align-items: center;
	justify-content: center;
	z-index: 222
}

.searchBtn image {
	width: 40rpx;
	height: 40rpx;
}

.title-line {
	width: 16px;
	height: 3px;
	background: rgba(255, 209, 1, 1);
