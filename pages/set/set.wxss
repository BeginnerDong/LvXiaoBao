.set{
	width: 100%;
	height: 50px;
	display: flex;
	border-bottom: 1px solid #F0E1DA;
}
.set-name{
	width: 90%;
	height: 50px;
	line-height: 50px;
	font-size: 16px;
	color: #323232;
	padding-left: 5%;
}
.set-img{
	width: 10%;
	height: 50px;
	line-height: 50px;
}
.set-img image{
	width: 20px;
	height: 20px;
}