.outbound{
	width: 100%;
	height: 400px;
}
.peo-name{
	width: 100%;
	height: 50px;
	display: flex;
	/* justify-content: center; */
	padding: 0 3%;
	line-height: 50px;
	border-bottom: 1px solid #E7E7E7;
}
.peo-con{
	width: 30%;
	height: 50px;
	font-size: 16px;
	color: #323232;
	opacity: 1;
	font-weight:500;
}
.peo-content{
	width: 70%;
	height: 50px;
	display: flex;
}
.peo-content input{
	width: 60%;
	height: 40px;
	border: none;
	font-size: 14px;
	color: #A1A1A1;
	opacity: 1;
}
.card-img{
	width: 20%;
	height: 50px;
	text-align:right;
	margin-right: 15%;
}
.card{
	width: 80%;
	
}
.card input{
	width: 100%;
	height: 40px;
	border: none;
	font-size: 14px;
	color: #A1A1A1;
	opacity: 1;
	line-height: 40px;
	padding-top: 7px;
}

.peo-content1 input{
	width: 100%;
	height: 40px;
	border: none;
	font-size: 14px;
	color: #A1A1A1;
	opacity: 1;
	line-height: 40px;
	padding-top: 7px;
}
.peo-content1{
	font-size: 14px;
	color:#A1A1A1;
}
.outbound-btn{
	width: 90%;
	height: 50px;
	background:linear-gradient(90deg,rgba(255,88,43,1) 0%,rgba(255,123,87,1) 100%);
	color: white;
	text-align: center;
	border-radius: 30px;
	line-height: 50px;
	margin-left: 5%;
	opacity: 0.3;
	margin-top: 20px;
	font-weight: bold;
	font-size: 14px;
}