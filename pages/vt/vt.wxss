.vt{
	width: 90%;
	height: 130px;
	border-radius: 10px;
	margin-left: 5%;
	background: #16C0A6;
	margin-top: 3%;
	box-shadow:0px 2px 3px 0px rgba(222,222,222,1);
	display: flex;
}
.vt-left{
	width: 80%;
	padding-left: 3%;
}
.left-img{
	width: 50%;
	display: flex;
}
.vt-imga{
	width: 50%;
	padding-top: 5%;
}
.vt-imga image{
	width: 80px;
	height: 40px;
	border: 1px dashed #F74D5F;
}
.vt-img{
	width: 40%;
	padding-top: 5%;
}
.vt-img image{
	width: 40px;
	height: 40px;
	border: 1px dashed #F74D5F;
}
.vt-con{
	width: 20%;
	height: 130px;
	line-height: 130px;
}
.vt-con image{
	width: 30px;
	height: 30px;
}
.vt-title{
	width: 100%;
	height: 40px;
	display: flex;
	margin-top: 20px;
}
.vt-name{
	color: #FFFFFF;
	height: 40px;
	font-size: 20pt;
}
.vt-namea{
	color: #FFFFFF;
	height: 40px;
	font-size: 13pt;
	text-decoration: line-through;
	margin-top: 10px;
	margin-left: 20px;
}
.set{
	width: 100%;
	height: 200px;
	margin-top: 20px;
}
.set-name{
	color: #323232;
	font-size: 16px;
	padding-left: 3%;
	width: 100%;
	height: 50px;
	line-height: 50px;
}
.set-con{
	width: 100%;
	height: 50px;
	display: flex;
	padding-left: 3%;
	line-height: 50px;
	border-bottom: 1px solid #A1A1A1;
}
.set-img{
	width: 10%;
	height: 50px;
}
.set-img image{
	width: 20px;
	height: 20px;
}
.set-title{
	width: 70%;
	height: 50px;
	margin-left: 3%;
	font-size: 15px;
	color: #323232;
}
.setimga{
	width: 20%;
	height: 50px;
}
.setimga image{
	width: 20px;
	height: 20px;
}
.specli{
	width: 100%;
	height: 130px;
	background: #E7E7E7;
}
.set-names{
	background: #E7E7E7;
	opacity: 0.6;
	font-size: 14px;
	padding-left: 3%;
	margin-top: 10px;
}
.count{
	width: 100%;
	height: 50px;
	display: flex;
}
.count-title{
	width: 20%;
	color: #323232;
	font-size: 13px;
	height: 50px;
	line-height: 50px;
	padding-left: 3%;
	text-align: center;
}
.count-con{
	width: 20%;
	color: #FF582B;
	font-size: 14pt;
	height: 50px;
	line-height: 50px;
	text-align: left;
}
.count-btn{
	width: 50%;
	height: 40px;
	border-radius: 30px;
	background: #FF582B;
	color: white;
	text-align: center;
	line-height: 40px;
	margin-top: 2%;
	margin-left: 4%;
}