page{
	background: #f5f5f5;
}
.set{
	width: 100%;
	height: 100rpx;
	display: flex;
	background: white;
}
.set-name{
	width: 90%;
	height: 100rpx;
	line-height: 100rpx;
	font-size: 36rpx;
	color: #323232;
	padding-left: 5%;
	font-weight: 500;
  box-sizing: border-box;
}
.set-img{
	width: 10%;
	height: 100rpx;
	line-height: 100rpx;
}
.set-img image{
	width: 40rpx;
	height: 40rpx;
}
.teax{
	padding:20rpx 5%;
	margin-top: 20rpx;
	background: white;
}
textarea{
	width: 100%;
	min-height:220rpx;
	font-size: 28rpx;
	color: #333;
  line-height: 40rpx;
  box-sizing: border-box;
}
.mobile{
  padding: 0 5%;
	height: 100rpx;
	display: flex;
	margin-top: 20rpx;
	background: white;
  justify-content: space-between;
}
.mobile-name{
	height:100rpx;
	line-height: 100rpx;
	color: #323232;
	font-size: 32rpx;
	font-weight: 500;
}
.mobile-con{
	width: 20%;
	height: 100rpx;
	line-height: 100rpx;
	color: #A1A1A1;
	font-size:28rpx;	
  text-align: right;
}
.upload{
	padding: 0 5%;
	margin-top: 20rpx;
	background: white;
}
.up-img{
	width: 100%;
	display: flex;
	margin-top: 20rpx;
	flex-wrap: wrap;
	padding-bottom: 40rpx;
}
.up-img .img{
	width: 200rpx;
	height: 200rpx;
	text-align: center;
	line-height: 200rpx;
	background: #F4F3F3;
	margin-top: 12px;
	margin-right: 12px;
	}
.img image{
	width: 40rpx;
	height: 40rpx;
}
.btn{
	width: 100%;
	margin-top: 40rpx;
	text-align: center;
}
.btn span{
	width: 600rpx;
	height: 88rpx;
	line-height: 88rpx;
	display: inline-block;
	color: white;
	background: #FF7B57;
	text-align: center;
	border-radius: 60rpx;
	opacity: 0.4;
  margin: 80rpx auto;
}