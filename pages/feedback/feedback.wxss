page{
	background: #E7E7E7;
}
.set{
	width: 100%;
	height: 50px;
	display: flex;
	border-bottom: 1px solid #F0E1DA;
	background: white;
}
.set-name{
	width: 90%;
	height: 50px;
	line-height: 50px;
	font-size: 16px;
	color: #323232;
	padding-left: 5%;
}
.set-img{
	width: 10%;
	height: 50px;
	line-height: 50px;
}
.set-img image{
	width: 20px;
	height: 20px;
}
.teax{
	width: 100%;
	height: 200px;
	margin-top: 10px;
	background: white;
}
textarea{
	width: 100%;
	height: 200px;
	font-size: 15px;
	padding-left: 10px;
	color: #C0C0C0;
	padding-top: 10px;
}
.mobile{
	width: 100%;
	height: 50px;
	display: flex;
	margin-top: 10px;
	background: white;
}
.mobile-name{
	width: 80%;
	height: 50px;
	line-height: 50px;
	color: #323232;
	font-size: 16px;
	padding-left: 5%;
}
.mobile-con{
	width: 20%;
	height: 50px;
	line-height: 50px;
	color: #A1A1A1;
	font-size: 14px;	
}
.upload{
	width: 100%;
	height: 280px;
	margin-top: 10px;
	background: white;
}
.up-img{
	width: 100%;
	height: 100px;
	display: flex;
	margin-top: 10px;
}
.up-img .img{
	width: 100px;
	height: 100px;
	text-align: center;
	line-height: 100px;
	background: #C0C0C0;
	margin-left: 3%;
	}
.img image{
	width: 20px;
	height: 20px;
}
.btn{
	width: 100%;
	height: 40px;
}
.btn span{
	width: 300px;
	height: 50px;
	line-height: 50px;
	display: inline-block;
	color: white;
	background: #FF7B57;
	margin-left: 5%;
	text-align: center;
	border-radius: 30px;
	opacity: 0.4;

}