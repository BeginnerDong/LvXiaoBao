page{
	background: #f5f5f5;
}
.set{
	width: 100%;
	height: 50px;
	display: flex;
	background: white;
}
.set-name{
	width: 90%;
	height: 50px;
	line-height: 50px;
	font-size: 16px;
	color: #323232;
	padding-left: 5%;
	font-weight: 500;
}
.set-img{
	width: 10%;
	height: 50px;
	line-height: 50px;
}
.set-img image{
	width: 20px;
	height: 20px;
}
.teax{
	padding:20rpx 5%;
	height: 400rpx;
	margin-top: 10px;
	background: white;
}
textarea{
	width: 100%;
	height:100%;
	font-size: 26rpx;
	color: #333;
  line-height: 40rpx;
}
.mobile{
	width: 100%;
	height: 100rpx;
	display: flex;
	margin-top: 20rpx;
	background: white;
}
.mobile-name{
	height:100rpx;
	line-height: 100rpx;
	color: #323232;
	font-size: 32rpx;
	padding-left: 5%;
	font-weight: 500;
}
.mobile-con{
	width: 20%;
	height: 100rpx;
	line-height: 100rpx;
	color: #A1A1A1;
	font-size:28rpx;	
}
.upload{
	width: 100%;
	margin-top: 20rpx;
	background: white;
}
.up-img{
	width: 100%;
	display: flex;
	margin-top: 20rpx;
	flex-wrap: wrap;
	padding-bottom: 40rpx;
}
.up-img .img{
	width: 200rpx;
	height: 200rpx;
	text-align: center;
	line-height: 200rpx;
	background: #F4F3F3;
	margin-left: 3%;
	}
.img image{
	width: 40rpx;
	height: 40rpx;
}
.btn{
	width: 100%;
	margin-top: 40rpx;
	text-align: center;
}
.btn span{
	width: 600rpx;
	height: 88rpx;
	line-height: 88rpx;
	display: inline-block;
	color: white;
	background: #FF7B57;
	text-align: center;
	border-radius: 60rpx;
	opacity: 0.4;
  margin: 80rpx auto;
}