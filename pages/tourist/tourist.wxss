page{
	background:#F4F3F3;
}
.tourist{
	width: 100%;
	height: 400px;
	padding-top: 2%;
	z-index: 111;
}
.saler{
	width:87%;
	margin: 0 auto;
	background: #fff;
	box-shadow:0px 2px 3px 0px rgba(222,222,222,1);
	border-radius:12px;
	height:200rpx;
	margin-top: 24px;
	padding-left: 3%;
}
.phone{
	width:40px;
	height:40px;
	border-radius:50%;
	display: flex;
	align-items: center;
	justify-content: center;
}
.phone image{
	width:30rpx;
	height:30rpx;
}
.left{
	width: 13%;
}
.left image{
	width: 20px;
	height:20px;
}
.middle{
	width:60%
}
.middle image{
	width:24rpx;
	height:24rpx
}
.right{
	width:27%
}
.phoneCall{
	width: 100%;
	height:100%;
	position: fixed;
	top:0;
	background:rgba(0,0,0,1);
	opacity:0.6;
	z-index:9999
}
.card{
	width:100%;
	font-size: 13px;
	color: #A1A1A1;
	margin-top: 4%;
}
.bottom{
	width: 100%;
	height: 50px;
	display: flex;
	position: fixed;
	bottom: 0;
	background: #E7E7E7;
}
.bottom-left{
	width: 60%;
	height: 50px;
	display: flex;
}
.bott{
	width: 30%;
	text-align: center;
	margin-top: 5px;
}
.left-img{
	width: 100%;
	height: 20px;
}
.left-img image{
	width: 20px;
	height: 20px;
}
.left-cn{
	font-size: 12px;
	color: #323232;
	margin-top: 5px;
	font-weight: 500;
}
.bott-btn{
	width: 40%;
	height: 40px;
	color: white;
	background:linear-gradient(90deg,rgba(255,88,43,1) 0%,rgba(255,123,87,1) 100%);
	text-align: center;
	line-height: 40px;
	border-radius: 30px;
	font-size: 17px;
	margin-top: 5px;
	font-weight:bold;
	margin-right: 5px;
}
.visi{
	width: 100%;
	height: 80%;
	position: fixed;
	bottom:20%;
	background: #000000;
	opacity: 0.6;
	z-index: 222;
}
.visitors{
	width: 100%;
	height: 25%;
	position: fixed;
	bottom:0%;
	background: #FFFFFF;
	border-radius: 20px;
	z-index: 333;
}
.visi-name{
	width: 100%;
	font-size: 16px;
	font-family:PingFang-SC-Bold;
	font-weight:bold;
	color:rgba(50,50,50,1);
	position: relative;
	z-index:1;
	padding-top: 3px;
	height: 50px;
	line-height: 50px;
	padding-left: 5%;
	border-radius: 30px;
}
.title-line {
	width: 16px;
	height: 3px;
	background: rgba(255, 209, 1, 1);
	border-radius: 2px;
	position:absolute;
	z-index:-1;
	left:5%;
	bottom: 17px;
}
.visi-name1{
	width: 100%;
	height: 50px;
	display: flex;
	line-height: 50px;
	justify-content: center;
	border-bottom: 1px solid #E7E7E7;
}
.visi-titlw{
	width: 90%;
	height: 50px;
	font-size: 16px;
	color: #323232;
	padding: 0 3%;
	font-weight:bold;
}
.visi-img{
	width: 10%;
	height: 50px;
	text-align: center;
}
.visi-img image{
	width: 20px;
	height: 20px;
}