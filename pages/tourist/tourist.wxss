page{
	background:#f5f5f5;
}
.tourist{
	width: 100%;
	padding-top: 2%;
	z-index: 111;
}
.saler{
	width:87%;
	margin: 0 auto;
	background: #fff;
	box-shadow:0px 4rpx 6rpx 0px rgba(222,222,222,1);
	border-radius:24rpx;
	height:200rpx;
	margin-top: 20rpx;
	padding-left: 3%;
}
.phone{
	width:80rpx;
	height:80rpx;
	border-radius:50%;
	display: flex;
	align-items: center;
	justify-content: center;
}
.phone image{
	width:30rpx;
	height:30rpx;
}
.left{
	width: 13%;
}
.left image{
	width:40rpx;
	height:40rpx;
}
.middle{
	width:60%
}
.middle image{
	width:24rpx;
	height:24rpx
}
.right{
	width:27%
}
.phoneCall{
	width: 100%;
	height:100%;
	position: fixed;
	top:0;
	background:rgba(0,0,0,1);
	opacity:0.6;
	z-index:9999
}
.card{
	width:100%;
	font-size: 26rpx;
	color: #A1A1A1;
	margin-top: 4%;
}
.bottom{
	width: 100%;
	height: 120rpx;
  padding: 0 20rpx;
  box-sizing: border-box;
	display: flex;
  justify-content: space-around;
  align-items: center;
	position: fixed;
	bottom: 0;
	background: #E7E7E7;
}
.bottom-left{
	width: 60%;
	display: flex;
}
.bott{
	width: 30%;
	text-align: center;
}
.left-img{
	width: 100%;
	height: 40rpx;
}
.left-img image{
	width: 40rpx;
	height: 40rpx;
}
.left-cn{
	font-size: 24rpx;
	color: #323232;
	margin-top: 10rpx;
	font-weight: 500;
}
.bott-btn{
	width: 40%;
	height: 80rpx;
	color: white;
	background:linear-gradient(90deg,rgba(255,88,43,1) 0%,rgba(255,123,87,1) 100%);
	text-align: center;
	line-height: 80rpx;
	border-radius: 60rpx;
	font-size: 34rpx;
	font-weight:bold;
}
.visi{
	width: 100%;
	height: 80%;
	position: fixed;
	bottom:20%;
	background: #000000;
	opacity: 0.6;
	z-index: 222;
}
.visitors{
	width: 100%;
	position: fixed;
	bottom:0;
  left:0;
	background: #FFFFFF;
	border-radius: 40rpx;
	z-index: 333;
}
.visi-name{
	width: 100%;
	font-size: 32rpx;
	font-family:PingFang-SC-Bold;
	font-weight:bold;
	color:rgba(50,50,50,1);
	position: relative;
	z-index:1;
	padding-top: 6rpx;
	height: 100rpx;
	line-height: 100rpx;
	padding-left: 5%;
	border-radius: 60rpx;
}
.title-line {
	width: 32rpx;
	height: 6rpx;
	background: rgba(255, 209, 1, 1);
	border-radius: 4rpx;
	position:absolute;
	z-index:-1;
	left:5%;
	bottom: 34rpx;
}
.visi-name1{
	width: 100%;
	height: 100rpx;
	display: flex;
	line-height: 100rpx;
	justify-content: center;
	border-bottom: 2rpx solid #E7E7E7;
}
.visi-titlw{
	width: 90%;
	height: 100rpx;
	font-size: 32rpx;
	color: #323232;
	padding: 0 3%;
	font-weight:bold;
}
.visi-img{
	width: 10%;
	height: 100rpx;
	text-align: center;
}
.visi-img image{
	width: 40rpx;
	height: 40rpx;
}