	background: white;
}
.basic{
	width: 50%;
	height: 100rpx;
	line-height: 100rpx;
	color: #323232;
	font-size: 32rpx;
	text-align: center;
	color:rgba(50,50,50,1);
	position: relative;
	z-index:1;
}
.title-line {
	width: 32rpx;
	height: 6rpx;
	background: rgba(255, 209, 1, 1);
