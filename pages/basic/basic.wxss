	position:absolute;
	z-index:-1;
	left:35%;
	bottom: 34rpx;
}
.travel{
	width: 50%;
	height: 100rpx;
	line-height: 100rpx;
	color: #323232;
	font-size: 32rpx;
	text-align: center;
	font-weight: 600;
	font-family:PingFang-SC-Bold;
	color:rgba(50,50,50,1);
	position: relative;
	z-index:1;
	padding-left: 3%;
