.top{
	width: 100%;
	height: 300px;
	background: #FFC900;
	display: flex;
}
.top-img{
	width: 60%;
	height: 200px;
	text-align: right;
	margin-top: 10px;
}
.top-img image{
	width: 80px;
	height: 80px;
	border-radius: 50%;
}
.top-imgs{
	width: 25%;
	height: 200px;
	margin-left: 5%;
	margin-top: 10px;
}
.top-imgs image{
	width: 80px;
	height: 80px;
	border: 1px dashed #000000;
}
.center{
	width: 90%;
	height: 130px;
	border-radius: 10px;
	margin-left: 5%;
	position: absolute;
	top: 20%;
	background: white;
	text-align: center;
	padding-top: 5%;
}
.center-name{
	color: #323232;
	font-size: 16pt;
	font-weight: 600;
}
.center-names{
	width: 100%;
	height: 50px;
	margin-top: 10px;
}
.nam{
	width: 80px;
	height: 30px;
	display: flex;
	background: #F6D7A0;
	border-radius: 10px;
	line-height: 30px;
	margin-left: 35%;
}
.name{
	font-size: 13px;
	font-weight: bold;
	color: #9C793D;
	margin-left: 3%;
}
.name-img{
	width: 40%;
	height: 30px;
	text-align: right;
}
.name-img image{
	width: 15px;
	height: 15px;
	border: 1px dashed #000000;
}
.name{
	width: 50%;
	height: 50px;
	text-align: left;
}
.people{
	width: 100%;
	height: 50px;
	display: flex;
	line-height: 50px;
	border: 1px dashed #F4F3F3;
}
.peo-name{
	width: 60%;
	height: 40px;
	line-height: 40px;
	color: #585858;
	font-size: 16px;
	padding-left: 4%;
	
}
.peo-con{
	width: 40%;
	color: #323232;
	font-size: 16px;
	line-height: 40px;
	text-align: right;
	margin-right: 5%;
	font-weight: bold;
}
.peo-namea{
	width: 40%;
	height: 40px;
	line-height: 40px;
	color: #585858;
	font-size: 16px;
	padding-left: 4%;
}
.peo-cona{
	width:60%;
	color: #323232;
	font-size: 16px;
	line-height: 40px;
	font-weight: bold;
}
.btn{
	width: 100%;
	height: 40px;
	margin-top: 20px;
}
.btn span{
	width: 300px;
	height: 50px;
	line-height: 50px;
	display: inline-block;
	color: white;
	background: #FF7B57;
	margin-left: 10%;
	text-align: center;
	border-radius: 30px;
	font-size: 17px;
	font-weight: bold;
}