page{
	background: #F4F3F3;
}
.jounery{
	width: 90%;
	height: 500px;
	box-shadow:0px 2px 3px 0px rgba(222,222,222,1);
	background: white;
	border-radius: 10px;
	margin-top: 10px;
	padding-top: 10px;
	color: #323232;
	margin-left: 5%;
}
.jo{
	width: 90%;
	height: 390px;
}
.joun{
	padding-left: 5%;
}
.joun-con{
	width: 100%;
	height: 360px;
	font-size: 15px;
	color: #323232;
	opacity: 1;
	margin-top: 4%;
	line-height: 25px;
	font-size:15px;
	font-family:PingFang-SC-Regular;
	font-weight:400;
	padding-left: 5%;
	padding-right: 5%;
}
.joun-con textarea{
  width: 100%;
  height: 100%;
  display: block;
}
.joun-btn{
	width: 90%;
  margin: 0 auto;
	height: 100px;
	display: flex;
	margin-top: 20px;
}
.joun-left{
	width: 50%;
	height: 80px;
	text-align: center;
}
.joun-top{
	width: 100%;
	height: 40px;
}
.joun-top image{
	width: 20px;
	height: 20px;
}
.jo-con{
	color: #EC6532;
	font-size: 14px;
	font-weight:500;
}
.jounery-btn{
	width: 90%;
	height: 50px;
	text-align: center;
}
.jounery-btn image{
	width: 25px;
	height: 25px;
}
