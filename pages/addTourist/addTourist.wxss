
page{
  padding-bottom: 160rpx;
}

.rightNav{
  position:fixed;
  width:40rpx;
  top:50%;
  transform: translateY(-50%);
  right:0;
  /* background-color:rgba(245,245,245,1); */
  z-index:2;
  text-align: center;
  line-height: 40rpx;
  padding: 10rpx 0;
  border-radius: 6rpx;
}
.rightNav .num{
  width: 30rpx; 
  height: 30rpx;
  line-height: 30rpx;
  margin: 8rpx auto;
  border-radius: 50%;
  font-size: 24rpx;
  color: #333;
  text-shadow: 0 0 4px rgba(0,0,0,0.1);
}
.rightNav .num.on{
  background: #FF582B;
  color: #fff;
}

.topAdd {
	width: 100%;
	background: #fff;
  padding:20rpx 4%;
  display: flex;
  justify-content: flex-start;
  align-items: center;
  flex-wrap: wrap;
  box-sizing: border-box;
}
.topAdd .lis{
  width: 140rpx;
  height: 60rpx;
  line-height: 60rpx;
  text-align: center;
  margin:10rpx 26rpx 10rpx 0;
  border-radius: 15rpx;
  font-size: 30rpx;
  border:2rpx solid #e7e7e7;
}



.client{
	width: 100%;
	height: 50px;
	display: flex;
	line-height: 50px;
	border-bottom: 1px solid #E7E7E7;
}
.client1{
	width: 100%;
  padding: 0 3%;
  box-sizing: border-box;
	display: flex;
	border-bottom: 1px solid #E7E7E7;
}
.client-name{
	width: 90%;
	color: #323232;
	opacity: 1;
	font-size: 16px;
	height: 50px;
	line-height: 50px;
	padding-left: 3%;
	font-weight: 600;
	font-family:PingFang-SC-Bold;
	color:rgba(50,50,50,1);
	position: relative;
	z-index:1;
	border-bottom: 1px solid #E7E7E7;
}
.title-line {
	width: 16px;
	height: 3px;
	background: rgba(255, 209, 1, 1);
	border-radius: 2px;
	position:absolute;
	z-index:-1;
	left:4%;
	bottom: 12px;
}
.client-img{
	width: 10%;
	height: 50px;
	line-height: 50px;
}
.client-img image{
	width: 15px;
	height: 15px;
}
.client-name{
	width: 100%;
	height: 50px;
	background: #f4f3f3;
}
.cli-title{
	width: 85%;
	color: #323232;
	font-size: 16px;
	padding:16rpx 0;
	line-height: 20px;
  box-sizing: border-box;
  margin-top: 10rpx;
} 
.selIcon{ 
  width: 10%; 
  display: flex;
  align-content: space-around;
  align-items: center;
  /* text-align: left; */
}
.selIcon image{ 
  width: 40rpx;
  height: 40rpx;
  display: block;
}
.title{
	color: #323232;
	font-weight: bold;
	font-size: 16px;
}
.title span{
	color: #C5C5C5;
	font-weight: 100;
	margin-left: 3%;
}
.title label{
	width: 40px;
	height: 20px;
	display: inline-block;
	border-radius: 30px;
	color: white;
	background: #FFA586;
	font-size: 10px;
	text-align: center;
	line-height: 20px;
	margin-left: 2%;
}
.mobile{
	color: #A1A1A1;
	opacity: 1;
	font-size: 13px;
  margin-top: 10rpx;
}
.subOkBtn{
  width: 100%;
  padding: 0 5%;
  box-sizing: border-box;
  height: 120rpx;
  display: flex;
  justify-content: space-around;
  align-items: center;
  position: fixed;
  bottom: 0;
  left: 0;
  z-index: 1;
  background: #fff;
  box-shadow: 0 -4rpx 8rpx rgba(0,0,0,0.1)
}
.subOkBtn .btn{
  width: 100%;
  line-height: 80rpx;
  height: 80rpx;
  text-align: center;
  color: #fff;
  font-size: 30rpx;
  border-radius: 50px;
  background: #ff754f;
}


