page{
	background: #f5f5f5;
}
.couchar{
	padding: 0 5%;
	background: white;
}
.cou{
	width: 100%;
	color: #323232;
	font-size: 38rpx;
	height: 80rpx;
	line-height: 80rpx;
	padding-top: 20rpx;
}
.couchar-inp{
	width: 100%;
	height: 80rpx;
	line-height: 80rpx;
	color: #323232;
	font-size: 60rpx;
	margin-top: 10rpx;
	font-weight: bold;
  padding-bottom: 30rpx;
}
.priceIput{ 
  font-size: 60rpx;
  line-height: 80rpx;
  height: 80rpx;
  padding-left: 20rpx;
}
.set{
  padding:0 5%;
	margin-top: 40rpx;
	background: white;
}
.set-name{
	height: 80rpx;
	color: #323232;
	font-size: 32rpx;
	font-weight: bold;
	font-family:PingFang-SC-Bold;
	font-weight:bold;
	color:rgba(50,50,50,1);
	position: relative;
	z-index:1;
	line-height: 80rpx;
	border-bottom: 2rpx solid #E7E7E7;
}
.title-line {
	width: 32rpx;
	height: 6rpx;
	background: rgba(255, 209, 1, 1);
	border-radius: 4rpx;
	position:absolute;
	z-index:-1;
	left:1%;
	bottom: 20rpx;
}
.set-con{
	width: 100%;
	height: 96rpx;
	display: flex;
	line-height: 96rpx;
	border-bottom: 2rpx solid #E7E7E7;
}
.set-img{
	width: 10%;
	text-align: center;
}
.set-img image{
	width: 36rpx;
	height: 36rpx;
}
.set-title{
	width: 70%;
	font-size: 32rpx;
	color: #333;
}
.setimga{
	width: 20%;
	height: 100rpx;
  text-align: center;
}
.setimga image{
	width: 36rpx;
	height: 36rpx;
}
.cou-btn{
	width: 100%;
	margin: 80rpx 0;
	text-align: center;
}
.cou-btn span{
	width: 80%;
	height: 88rpx;
	text-align: center;
	color: #F4F3F3;
	background: #FF582B;
	line-height: 88rpx;
	display: block;
	border-radius: 50rpx;
	font-weight: bold;
	font-size: 32rpx;
  margin: 0 auto;
}
