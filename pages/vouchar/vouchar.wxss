page{
	background: #E0E0E0;
}
.couchar{
	width: 100%;
	height: 100px;
	background: white;
}
.cou{
	width: 100%;
	padding-left: 3%;
	color: #323232;
	font-size: 16pt;
	height: 20px;
	line-height: 20px;
	padding-top: 2%;
}
.couchar-inp{
	width: 100%;
	padding-left: 3%;
	height: 20px;
	line-height: 20px;
	color: #323232;
	font-size: 30px;
	margin-top: 20px;
	font-weight: bold;
}
.set{
	width: 100%;
	height: 150px;
	margin-top: 20px;
	background: white;
}
.set-name{
	color: #323232;
	font-size: 16px;
	padding-left: 3%;
	width: 100%;
	height: 50px;
	line-height: 50px;
}
.set-con{
	width: 100%;
	height: 50px;
	display: flex;
	padding-left: 3%;
	line-height: 50px;
	border-bottom: 1px solid #A1A1A1;
}
.set-img{
	width: 10%;
	height: 50px;
}
.set-img image{
	width: 15px;
	height: 15px;
	border: 1px dashed #000000;
	margin-top: 10px;
}
.set-title{
	width: 70%;
	height: 50px;
	margin-left: 3%;
	font-size: 15px;
	color: #323232;
}
.setimga{
	width: 20%;
	height: 50px;
}
.setimga image{
	width: 20px;
	height: 20px;
}
.cou-btn{
	width: 100%;
	height: 40px;
	margin-top: 20px;
}
.cou-btn span{
	width: 300px;
	height: 40px;
	text-align: center;
	color: white;
	background: #FF7B57;
	line-height: 40px;
	display: inline-block;
	border-radius: 30px;
	margin-left: 15%;
}
.draw{
	width: 100%;
	height: 40px;
	display: flex;
	background: white;
}
.draw-top{
	width: 90%;
	height: 40px;
	line-height: 40px;
	color: #323232;
	font-size: 15px;
	padding-left: 5%;
	font-weight: 500;
}
.draw-imga{
	width: 10%;
	height: 40px;
	line-height: 40px;
}
.draw-imga image{
	width: 20px;
	height: 20px;
}
.drawa{
	width: 100%;
	height: 140px;
	background: white;
	margin-top: 10px;
}
.draw-topa{
	padding-left: 5%;
	width: 100%;
	height: 30px;
	line-height: 30px;
	border: 1px solid white;
	color: #323232;
	font-size: 16px;
	font-weight: bold;
}
.draw-cen{
	width: 100%;
	height: 50px;
	display: flex;
}
.couchar-inp{
	width: 70%;
}
.draw-ri{
	width: 30%;
	color: #5B669E;
	font-size: 11px;
	margin-top: 30px;
	font-weight: 400;
}
.draw-img{
	width: 100%;
	display: flex;
	height: 50px;
}
.draw-con{
	width: 90%;
	padding-left: 5%;
	color: #323232;
	font-size: 15px;
	margin-top: 20px;
	font-weight: bold;
}
.drimg{
	width: 10%;
}
.drimg image{
	width: 20px;
	height: 20px;
}