	margin-right: 15%;
}
.card{
	width: 80%;
	height: 40px;
	line-height: 40px;
	padding-top: 3%;
}
.card span{
	width: 80px;
	height: 20px;
	display: inline-block;
	border-radius: 12px;
	background: rgba(242,242,242,1);
	font-size: 12px;
	color: #585858;
	font-weight: 500;
	text-align: center;
	line-height: 20px;
}
.card input{
	width: 60%;
	height: 40px;
	border: none;
	font-size: 14px;
	color: #A1A1A1;
	opacity: 1;
	
}
.card-img image{
	width:15px;
