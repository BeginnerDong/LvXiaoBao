page{
	background: #F4F3F3;
}
.custom{
	width: 100%;
	height: 690px;
	background: white;
	padding-top: 10px;
}
.custom-name{
	width: 100%;
	height: 40px;
	padding-left: 5%;
	color: #323232;
	font-size: 16px;
	font-weight: bold;
	border-bottom: 1px solid #E7E7E7;
}
.peo-name{
	width: 100%;
	height: 50px;
