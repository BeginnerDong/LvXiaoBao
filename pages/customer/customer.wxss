page{
	background: #F4F3F3;
}
.custom{
	width: 100%;
	height: 700px;
	background: white;
	padding-top: 10px;
}
.custom-name{
	width: 100%;
	height: 40px;
	padding-left: 5%;
	color: #323232;
	font-size: 16px;
	font-weight: bold;
	border-bottom: 1px solid #E7E7E7;
	line-height:40px;
	font-family:PingFang-SC-Bold;
	color:rgba(50,50,50,1);
	position: relative;
	z-index:1;
}
.title-line {
	width: 16px;
	height: 3px;
	background: rgba(255, 209, 1, 1);
	border-radius: 2px;
	position:absolute;
	z-index:-1;
	left:4.5%;
	bottom: 12px;
}
.peo-name{
	width: 100%;
	height: 50px;
	display: flex;
	padding: 0 3%;
	line-height: 50px;
	border-bottom: 1px solid #E7E7E7;
}
.peo-con{
	width: 30%;
	height: 50px;
	font-size: 16px;
	color: #323232;
	font-weight: 500;
	padding-left: 5%;
}
.peo-content{
	width: 70%;
	height: 50px;
	display: flex;
	line-height: 50px;
}
.peo-content input{
	width: 60%;
	height: 40px;
	border: none;
	font-size: 10px;
	color: #A1A1A1;
	opacity: 1;
}
.peo-content input[placeholder]{
	color: #A1A1A1;
}
.card-img{
	width: 20%;
	height: 50px;
	text-align:right;
	margin-right: 15%;
}
.card{
	width: 80%;
	height: 40px;
	line-height: 40px;
	padding-top: 3%;
}
.card span{
	width: 80px;
	height: 20px;
	display: inline-block;
	border-radius: 12px;
	background: rgba(242,242,242,1);
	font-size: 12px;
	color: #585858;
	font-weight: 500;
	text-align: center;
	line-height: 20px;
}
.card input{
	width: 60%;
	height: 40px;
	border: none;
	font-size: 14px;
	color: #A1A1A1;
	opacity: 1;
	
}
.card-img image{
	width:15px;
	height: 15px;
}

.peo-content1{
	font-size: 14px;
	color: #323232;
	width: 70%;
	height: 50px;
	padding-top: 3%;
}
.cust{
	width: 100%;
	height: 70px;
	line-height: 45px;
	text-align: center;
	color: #585858;
	font-size: 13px;
	font-weight: bold;
}
.type{
	width: 100%;
	height: 350px;
	margin-top: 30px;
	background: white;
}
.type-name{
	width: 100%;
	height: 40px;
	padding-left: 5%;
	color: #323232;
	font-size: 16px;
	font-weight: bold;
	border-bottom: 1px solid #E7E7E7;
	line-height:40px;
	font-family:PingFang-SC-Bold;
	color:rgba(50,50,50,1);
	position: relative;
	z-index:1;
}
.photo{
	width: 100%;
	height: 120px;
	display: flex;
}
.phpto-text{
	width: 30%;
	height: 60px;
	color: #323232;
	line-height: 60px;
	font-size: 16px;
	padding-left:10%;
}
.photo-img{
	width: 100px;
	height: 100px;
	background: #F4F3F3;
	text-align: center;
	line-height: 100px;
}
.photo-img image{
	width: 20px;
	height: 20px;
}
.basic-name{
	width: 100%;
	height: 40px;
	line-height: 40px;
	display: flex;
	border-bottom: 1px solid #F4F3F3;
}
.title{
	width: 80%;
	margin-right: 5%;
	text-align: center;
	color: #323232;
	font-size: 16px;
	font-weight: 600;
}
.title-con{
	width: 70%;
	height: 40px;
	display: flex;
}
.name{
	width: 20%;
	color: #323232;
	font-size: 16px;
	padding-left: 7%;
	font-weight: 500;
}
.con{
	width: 70%;
	text-align: center;
	color: #323232;
	font-size: 16px;
	font-weight: 600;
	margin-left: 13%;
}
.con-img{
	width: 10%;
	margin-right: 5%;
}
.con-img image{
	width: 20px;
	height: 20px;
}
.con input{
	width: 60%;
	height: 40px;
	border: none;
	font-size: 14px;
	color: #A1A1A1;
	opacity: 1;
}
.title input{
	width: 60%;
	height: 40px;
	border: none;
	font-size: 14px;
	color: #A1A1A1;
	opacity: 1;
	text-align: right;
}
.photo{
	width: 100%;
	height: 140px;
	display: flex;
	border-bottom: 1px solid #E7E7E7;
}
.phpto-text{
	width: 30%;
	height: 100px;
	color: #323232;
	line-height: 100px;
	font-size: 16px;
	padding-left: 5%;
}
.photo-img{
	width: 100px;
	height: 100px;
	background: #F4F3F3;
	text-align: center;
	line-height: 100px;
	margin-top: 20px;
	border-radius: 10px;
}
.photo-img image{
	width: 20px;
	height: 20px;
}
.basic-btn{
	width: 100%;
	height: 40px;
	text-align: center;
	margin-top: 40px;
}
.basic-btn .basi-name{
	width: 327px;
	height: 48px;
	background:linear-gradient(90deg,rgba(255,88,43,1) 0%,rgba(255,123,87,1) 100%);
	color: white;
	border-radius: 30px;
	text-align: center;
	line-height: 48px;
	opacity: 0.3;
	font-weight: bold;
	margin-left: 6%;
}