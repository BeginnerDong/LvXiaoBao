<view>
	<view class="custom">
		<view class="custom-name">身份信息</view>
		<view class="peo-name">
			<view class="peo-con">姓名</view>
			<view class="peo-content1">
				<input type="text" placeholder="填写客户姓名" value="{{web_mainData.name}}"  data-key="name" bindblur="inputChange"/>
			</view>
		</view>
		<view class="peo-name">
			<view class="peo-con">姓名备注</view>
			<view class="peo-content1">
				<input type="text" placeholder="填写客户备注姓名" value="{{web_mainData.aliasName}}"  data-key="aliasName" bindblur="inputChange"/>
			</view>
		</view>
		<view class="peo-name">
			<view class="peo-con">客户级别</view>
			<view class="peo-content">
				<view class="card">
					
					<picker bindchange="educationChange" value="{{index}}"  range="{{levelData}}" style="width:100%;">
						<view style="font-size: 14px;color: #323232;">
							{{levelData[web_index]?levelData[web_index]:'选择客户级别'}}
						</view>
					</picker>
				</view>
				<view class="card-img"><image src="../../image/right.png"></image></view>
			</view>
		</view>
		<view class="peo-name">
			<view class="peo-con">手机</view>
			<view class="peo-content1">
				<input type="text" placeholder="填写客户手机"/>
			</view>
		</view>
		<view class="peo-name">
			<view class="peo-con">备用手机</view>
			<view class="peo-content1">
				<input type="text" placeholder="填写客户备用手机"/>
			</view>
		</view>
		<view class="peo-name">
			<view class="peo-con">性别</view>
			<view class="peo-content">
				<view class="card">
					<picker bindchange="changeLevel" value="{{index}}"  range="{{sexData}}" style="width:100%;">
						<view style="font-size: 14px;color: #323232;">
							{{sexData[web_sexIndex]?sexData[web_sexIndex]:'选择客户级别'}}
						</view>
					</picker>
				</view>
				<view class="card-img"><image src="../../image/right.png"></image></view>
			</view>
		</view>
		<view class="peo-name">
			<view class="peo-con">出生日期</view>
			<view class="peo-content1">
				<input type="text" placeholder="选择出生日期" />
			</view>
		</view>
		<view class="peo-name">
			<view class="peo-con">年龄</view>
			<view class="peo-content1">
				<input type="text" placeholder="填写客户年龄" value="{{web_mainData.age}}"  data-key="age" bindblur="inputChange"/>
			</view>
		</view>
		<view class="peo-name">
			<view class="peo-con">区域</view>
			<view class="peo-content">
				<view class="card">
					<picker  mode="region" bindchange="bindRegionChange" value="{{region}}" custom-item="{{customItem}}">
						<view style="color:#666;font-size:28rpx;">
							{{web_region?web_region:'请选择客户区域'}}
						</view>
					</picker>
				</view>
				<view class="card-img"><image src="../../image/right.png"></image></view>
			</view>
		</view>
		<view class="peo-name">
			<view class="peo-con">详细地址</view>
			<view class="peo-content1">
				<input type="text" placeholder="请与证件保持一致" value="{{web_mainData.address}}"  data-key="address" bindblur="inputChange"/>
			</view>
		</view>
		<view class="peo-name" style="height: 100px;">
			<view class="peo-con">标签</view>
			<view class="peo-content" >
				<view class="card">
					<span>来店里好几次</span>...
				</view>
				<view class="card-img"><image src="../../image/right.png"></image></view>
			</view>
		</view>
		<view class="cust" bindtap="intoPath" data-path="/pages/labels/labels">+添加新标签</view>
	</view>
	<view class="type">
		<view class="type-name">证件信息</view>
		<view class="basic-name">
			<view class="name">证件类型</view>
			<view class="title-con">
				<view class="con">
					<picker bindchange="changeCard" value="{{index}}"  range="{{cardData}}" style="width:100%;">
						<view style="font-size: 14px;color: #323232;">
							{{cardData[web_cardIndex]?cardData[web_cardIndex]:'选择证件类别'}}
						</view>
					</picker>
				</view>
				<view class="card-img">
					<image src="../../image/right.png"></image>
				</view>
			</view>
		</view>
		<view class="basic-name">
			<view class="name">证件号码</view>
			<view class="title">
				<input type="text" placeholder="填写证件号码" />
			</view>
		</view>
		<view class="basic-name">
			<view class="name">有效期至</view>
			<view class="title">
				<input type="text" placeholder="选择有效日期"/>
			</view>
		</view>
		<view class="photo">
			<view class="phpto-text">照片</view>
			<view class="photo-img">
				<image src="../../image/caram.png"></image>
			</view>
		</view>
	</view>
	<view class="basic-btn" bindtap="submit">
		<view class="basi-name">保存</view>
	</view>
</view>
