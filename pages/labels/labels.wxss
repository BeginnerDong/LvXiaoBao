page{
	background: #F4F3F3;
}
.top {
	width: 100%;
	
	background: white;
}
.search {
	width: 100%;
	padding: 20rpx 0;
}
.search input {
	width: 85%;
	background: #f4f4f4;
	border-radius: 40rpx;
	height: 80rpx;
	padding: 0 20rpx;
	position: relative;
	margin: 0 auto;
	font-size: 28rpx;
	font-family: PingFang-SC-Medium;
	font-weight: 500;
	
}

.searchBtn {
	width: 160rpx;
	height: 80rpx;
	background:#FFD101;
	border-radius: 0px 40rpx 40rpx 0px;
	position: absolute;
	right: 4.5%;
	top: 20rpx;
	display: flex;
	align-items: center;
	justify-content: center;
}

.searchBtn image {
	width: 50rpx;
	height: 26rpx;
}
.label-nav{
	width: 100%;
	display: flex;
	flex-wrap: wrap;
  padding-top: 30rpx;

}
.nav-item{
	width: 28%;
	height: 60rpx;
	text-align: center;
	line-height:60rpx;
	border-radius: 20rpx;
	margin-left: 3%;
	border: 2rpx solid #C5C5C5;
	color: #A1A1A1;
	font-size: 28rpx;
	background: white;
	font-weight: 500;
	margin-bottom: 20rpx;
}
.all-nav{
	width: 100%;
	margin-top:40rpx;
}
.nav-top{
	width: 100%;
	height: 80rpx;
	padding-left: 5%;
	color: #323232;
	font-size: 32rpx;
	font-weight: bold;
	line-height:80rpx;
	font-family:PingFang-SC-Bold;
	color:rgba(50,50,50,1);
	position: relative;
	z-index:1;
}
.title-line {
	width: 32rpx;
	height: 6rpx;
	background: rgba(255, 209, 1, 1);
	border-radius: 4rpx;
	position:absolute;
	z-index:-1;
	left:4.5%;
	bottom: 24rpx;
}
.nav-center{
	width: 100%;
	display: flex;
	flex-wrap: wrap;
}
.nav-bott{
	width: 100%;
	height: 100rpx;
	display: flex;
}
.add{
	width: 100%;
	height: 100rpx;
	text-align: center;
  margin-top: 80rpx;
}
.add span{
	width: 600rpx;
	height: 88rpx;
	display: inline-block;
	line-height: 88rpx;
	text-align: center;
	color: white;
	background:linear-gradient(90deg,rgba(255,88,43,1) 0%,rgba(255,123,87,1) 100%);
	font-weight: bold;
	font-size: 32rpx;
	border-radius:96rpx;
}