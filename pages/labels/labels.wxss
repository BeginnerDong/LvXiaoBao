.top {
	width: 100%;
	height: 100px;
}

.search {
	width: 100%;
	padding: 20rpx 0;
}

.search input {
	width: 85%;
	background: rgb(216, 212, 212);
	border-radius: 40rpx;
	height: 80rpx;
	padding: 0 20rpx;
	position: relative;
	margin: 0 auto;
	font-size: 14px;
	font-family: PingFang-SC-Medium;
	font-weight: 500;
	color: rgba(192, 192, 192, 1);
}

.searchBtn {
	width: 100rpx;
	height: 80rpx;
	background:white;
	border-radius: 0px 20px 20px 0px;
	position: absolute;
	right: 4.5%;
	top: 20rpx;
	display: flex;
	align-items: center;
	justify-content: center;
}

.searchBtn image {
	width: 40rpx;
	height: 40rpx;
}
.label-nav{
	width: 100%;
	height: 40px;
	display: flex;
}
.nav-item{
	width: 28%;
	height: 30px;
	text-align: center;
	line-height: 30px;
	border-radius: 10px;
	margin-left: 3%;
	border: 1px solid #323232;
	color: #A1A1A1;
	font-size: 14px;
}
.all-nav{
	width: 100%;
	height: 100px;
	margin-top: 20px;
}
.nav-top{
	width: 100%;
	height: 30px;
	padding-left: 5%;
	color: #323232;
	font-size: 16px;
	
}
.nav-center{
	width: 100%;
	height: 50px;
	display: flex;
}
.nav-bott{
	width: 100%;
	height: 50px;
	display: flex;
}