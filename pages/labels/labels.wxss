page{
	background: #F4F3F3;
}
.top {
	width: 100%;
	
	background: white;
}
.search {
	width: 100%;
	padding: 20rpx 0;
}
.search input {
	width: 85%;
	background: #f4f4f4;
	border-radius: 40rpx;
	height: 80rpx;
	padding: 0 20rpx;
	position: relative;
	margin: 0 auto;
	font-size: 14px;
	font-family: PingFang-SC-Medium;
	font-weight: 500;
	
}

.searchBtn {
	width: 80px;
	height: 40px;
	background:#FFD101;
	border-radius: 0px 20px 20px 0px;
	position: absolute;
	right: 4.5%;
	top: 20rpx;
	display: flex;
	align-items: center;
	justify-content: center;
}

.searchBtn image {
	width: 50rpx;
	height: 30rpx;
}
.label-nav{
	width: 100%;
	
	display: flex;
	flex-wrap: wrap;

}
.nav-item{
	width: 28%;
	height: 30px;
	text-align: center;
	line-height: 30px;
	border-radius: 10px;
	margin-left: 3%;
	border: 1px solid #C5C5C5;
	color: #A1A1A1;
	font-size: 14px;
	background: white;
	font-weight: 500;
	margin-bottom: 10px;
}
.all-nav{
	width: 100%;
	height: 300px;
	margin-top: 20px;
}
.nav-top{
	width: 100%;
	height: 40px;
	padding-left: 5%;
	color: #323232;
	font-size: 16px;
	font-weight: bold;
	line-height:40px;
	font-family:PingFang-SC-Bold;
	color:rgba(50,50,50,1);
	position: relative;
	z-index:1;
}
.title-line {
	width: 16px;
	height: 3px;
	background: rgba(255, 209, 1, 1);
	border-radius: 2px;
	position:absolute;
	z-index:-1;
	left:4.5%;
	bottom: 12px;
}
.nav-center{
	width: 100%;
	height: 50px;
	display: flex;
	
	flex-wrap: wrap;
}
.nav-bott{
	width: 100%;
	height: 50px;
	display: flex;
}
.add{
	width: 100%;
	height: 50px;
	text-align: center;
}
.add span{
	width: 300px;
	height: 48px;
	display: inline-block;
	line-height: 48px;
	text-align: center;
	color: white;
	background:linear-gradient(90deg,rgba(255,88,43,1) 0%,rgba(255,123,87,1) 100%);
	font-weight: bold;
	font-size: 17px;
	border-radius:48px;
}