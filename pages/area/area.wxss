page{
	background: #F4F3F3;
}
.all-nav{
	width: 100%;
	
	background: white;
	padding-top: 40rpx;
}
.nav-top{
	width: 100%;
	font-size: 32rpx;
	font-family:PingFang-SC-Bold;
	font-weight:bold;
	color:rgba(50,50,50,1);
	position: relative;
	z-index:1;
	padding-top: 6rpx;
	height: 80rpx;
	line-height: 80rpx;
	padding-left: 5%;
	
}
.title-line {
	width: 32rpx;
	height: 6rpx;
	background: rgba(255, 209, 1, 1);
	border-radius: 2px;
	position:absolute;
	z-index:-1;
	left:5%;
	bottom: 34rpx;
}
.nav-center{
	width: 100%;
	display: flex;
	padding: 20rpx 0;
	flex-wrap: wrap;
}
.nav-item{
	width: 28%;
	height: 44rpx;
	text-align: center;
	line-height: 44rpx;
	border-radius: 20rpx;
	margin-left: 3%;
	border: 2rpx solid #C5C5C5;
	color: #A1A1A1;
	font-size: 26rpx;
	font-weight: 500;
	margin-top: 20rpx;
}
.nav-bott{
	width: 100%;
	height: 100rpx;
	display: flex;
	margin-top: 20rpx;
}
.area-btn{
	width: 100%;
	height: 100rpx;
	margin-top: 400rpx;
}
.area-btn span{
	width: 600rpx;
	height: 100rpx;
	display: inline-block;
	background: #FF582B;
	color: white;
	text-align: center;
	line-height: 100rpx;
	margin-left: 10%;
	border-radius: 60rpx;
}