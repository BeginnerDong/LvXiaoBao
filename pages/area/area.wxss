page{
	background: #F4F3F3;
}
.all-nav{
	width: 100%;
	
	background: white;
	padding-top: 20px;
}
.nav-top{
	width: 100%;
	font-size: 16px;
	font-family:PingFang-SC-Bold;
	font-weight:bold;
	color:rgba(50,50,50,1);
	position: relative;
	z-index:1;
	padding-top: 3px;
	height: 40px;
	line-height: 40px;
	padding-left: 5%;
	
}
.title-line {
	width: 16px;
	height: 3px;
	background: rgba(255, 209, 1, 1);
	border-radius: 2px;
	position:absolute;
	z-index:-1;
	left:5%;
	bottom: 17px;
}
.nav-center{
	width: 100%;
	display: flex;
	margin-top: 10px;
	flex-wrap: wrap;
}
.nav-item{
	width: 28%;
	height: 32px;
	text-align: center;
	line-height: 32px;
	border-radius: 10px;
	margin-left: 3%;
	border: 1px solid #C5C5C5;
	color: #A1A1A1;
	font-size: 14px;
	font-weight: 500;
	margin-top: 10rpx;
}
.nav-bott{
	width: 100%;
	height: 50px;
	display: flex;
	margin-top: 10px;
}
.area-btn{
	width: 100%;
	height: 50px;
	margin-top: 20px;
}
.area-btn span{
	width: 300px;
	height: 50px;
	display: inline-block;
	background: #FF582B;
	color: white;
	text-align: center;
	line-height: 50px;
	margin-left: 10%;
	border-radius: 30px;
}