.all-nav{
	width: 100%;
	height: 100px;
	margin-top: 20px;
}
.nav-top{
	width: 100%;
	height: 30px;
	padding-left: 5%;
	color: #323232;
	font-size: 16px;
	
}
.nav-center{
	width: 100%;
	height: 50px;
	display: flex;
}
.nav-item{
	width: 28%;
	height: 30px;
	text-align: center;
	line-height: 30px;
	border-radius: 10px;
	margin-left: 3%;
	border: 1px solid #323232;
	color: #A1A1A1;
	font-size: 14px;
}
.nav-bott{
	width: 100%;
	height: 50px;
	display: flex;
}
.area-btn{
	width: 100%;
	height: 50px;
}
.area-btn span{
	width: 300px;
	height: 50px;
	display: inline-block;
	background: #FF582B;
	color: white;
	text-align: center;
	line-height: 50px;
	margin-left: 10%;
	border-radius: 30px;
}