
.top {
	width: 100%;
	height: 160rpx;
	border-bottom: 2rpx solid #e7e7e7;
	background: rgb(248, 247, 244);
}

.search {
	width: 100%;
	padding: 40rrpx 0;
}
.search input {
	width: 85%;
	background: rgb(216, 212, 212);
	border-radius: 40rpx;
	height: 80rpx;
	padding: 0 20rpx;
	position: relative;
	margin: 0 auto;
	font-size: 14px;
	font-family: PingFang-SC-Medium;
	font-weight: 500;
	color: rgba(192, 192, 192, 1);
}

.searchBtn {
	width: 100rpx;
	height: 80rpx;
	background:#FFD101;
	border-radius: 0px 40rpx 40rpx 0px;
	position: absolute;
	right: 4.5%;
	top: 20rpx;
	display: flex;
	align-items: center;
	justify-content: center;
}

.searchBtn image {
	width: 40rpx;
	height: 40rpx;
}
.agents{
	height: 100rpx;
	line-height: 100rpx;
	color: #585858;
	font-size: 32rpx;
	padding:0 5%;
	background: #F4F3F3;
}
.agents-name{
	width: 90%;
  margin: 0 auto;
	/* height: 120rpx; */
	padding: 30rpx 0;
	border-bottom: 2rpx solid #E0E0E0;
}
.agents-title{
	width: 100%;
  line-height: 40rpx;
	color: #323232;
	font-size: 32rpx;
	font-weight: bold;
}
.agent-left{
	width: 100%;
	display: flex;
	margin-top: 10rpx;
}
.left-name{
	color: #A1A1A1;
	font-size: 26rpx;
	margin-right: 2%;
}