
.top {
	width: 100%;
	height: 80px;
	border-bottom: 1px solid gray;
	background: rgb(248, 247, 244);
}

.search {
	width: 100%;
	padding: 20rpx 0;
}
.search input {
	width: 85%;
	background: rgb(216, 212, 212);
	border-radius: 40rpx;
	height: 80rpx;
	padding: 0 20rpx;
	position: relative;
	margin: 0 auto;
	font-size: 14px;
	font-family: PingFang-SC-Medium;
	font-weight: 500;
	color: rgba(192, 192, 192, 1);
}

.searchBtn {
	width: 100rpx;
	height: 80rpx;
	background:#FFD101;
	border-radius: 0px 20px 20px 0px;
	position: absolute;
	right: 4.5%;
	top: 20rpx;
	display: flex;
	align-items: center;
	justify-content: center;
}

.searchBtn image {
	width: 40rpx;
	height: 40rpx;
}
.agents{
	width: 100%;
	height: 50px;
	line-height: 50px;
	color: #585858;
	font-size: 16pt;
	padding-left: 5%;
	background: #F4F3F3;
}
.agents-name{
	width: 100%;
	height: 60px;
	padding-left: 5%;
	padding-top: 5%;
	border-bottom: 1px solid #E0E0E0;
}
.agents-title{
	width: 100%;
	height: 20px;
	color: #323232;
	font-size: 16px;
	font-weight: bold;
}
.agent-left{
	width: 100%;
	height: 30px;
	display: flex;
	margin-top: 10px;
}
.left-name{
	color: #A1A1A1;
	font-size: 13px;
	margin-left: 2%;
}