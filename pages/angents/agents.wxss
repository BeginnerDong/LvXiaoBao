

.rightNav{
  position:fixed;
  width:40rpx;
  top:50%;
  transform: translateY(-50%);
  right:0;
  /* background-color:rgba(245,245,245,1); */
  z-index:2;
  text-align: center;
  line-height: 40rpx;
  padding: 10rpx 0;
  border-radius: 6rpx;
}
.rightNav .num{
  width: 30rpx; 
  height: 30rpx;
  line-height: 30rpx;
  margin: 8rpx auto;
  border-radius: 50%;
  font-size: 24rpx;
  color: #333;
  text-shadow: 0 0 4px rgba(0,0,0,0.1);
}
.rightNav .num.on{
  background: #FF582B;
  color: #fff;
}

.top {
	width: 100%;
	height: 160rpx;
	border-bottom: 2rpx solid #e7e7e7;
	background: rgb(248, 247, 244);
  display: flex;
  align-items: center;
}

.search {
	width: 85%;
  margin: 0 auto;
  padding: 0 100rpx 0 0;
  position: relative;
  box-sizing: border-box;

}
.search input {
	width: 100%;
	background: rgb(216, 212, 212);
	border-radius: 40rpx 0 0 40rpx;
	height: 80rpx;
	padding: 0 20rpx;
	position: relative;
	margin: 0 auto;
	font-size: 14px;
	font-family: PingFang-SC-Medium;
	font-weight: 500;
	color: rgba(192, 192, 192, 1);
  box-sizing: border-box;
}

.searchBtn {
	width: 100rpx;
	height: 80rpx;
	background:#FFD101;
	border-radius: 0px 40rpx 40rpx 0px;
	position: absolute;
	right: 0;
	top: 0rpx;
	display: flex;
	align-items: center;
	justify-content: center;
}

.searchBtn image {
	width: 40rpx;
	height: 40rpx;
}
.agents{
	height: 100rpx;
	line-height: 100rpx;
	color: #585858;
	font-size: 32rpx;
	padding:0 5%;
	background: #F4F3F3;
}
.agents-name{
	width: 90%;
  margin: 0 auto;
	/* height: 120rpx; */
	padding: 30rpx 0;
	border-bottom: 2rpx solid #E0E0E0;
}
.agents-title{
	width: 100%;
  line-height: 40rpx;
	color: #323232;
	font-size: 32rpx;
	font-weight: bold;
}
.agent-left{
	width: 100%;
	display: flex;
	margin-top: 10rpx;
}
.left-name{
	color: #A1A1A1;
	font-size: 26rpx;
	margin-right: 2%;
}