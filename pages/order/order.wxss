page {
	background: #F4F3F3;
}

.top {
	width: 100%;
	background: #FFD101;
	z-index: 222;
}

.search {
	width: 100%;
	padding: 20rpx 0;
}

.search input {
	width: 85%;
	background: #fff;
	border-radius: 40rpx;
	height: 80rpx;
	padding: 0 20rpx;
	position: relative;
	margin: 0 auto;
	font-size: 14px;
	font-family: PingFang-SC-Medium;
	font-weight: 500;
	color: rgba(192, 192, 192, 1);
}

.searchBtn {
	width: 100rpx;
	height: 80rpx;
	background: rgba(50, 50, 50, 1);
	border-radius: 0px 20px 20px 0px;
	position: absolute;
	right: 4.5%;
	top: 20rpx;
	display: flex;
	align-items: center;
	justify-content: center;
	z-index: 222
}

.searchBtn image {
	width: 40rpx;
	height: 40rpx;
}

.title-line {
	width: 16px;
	height: 3px;
	background: rgba(255, 209, 1, 1);
	border-radius: 2px;
	position:absolute;
	bottom:10rpx;
	z-index:-1;
	left:30%;

}

.nav-top {
	width: 100%;
	height: 100rpx;
	background: #fff;
	z-index: 222;
}

.nav-top-item {
	width: 20%;
	text-align: center;
	font-size: 15px;
	font-family: PingFang-SC-Medium;
	font-weight: 500;
	color: rgba(50, 50, 50, 1);
	line-height: 50rpx;
	height:50rpx;
	position: relative;
	z-index:222
}
.nav-top-item-active{
	font-weight: 700;
}
.nav-top-item image{
	width:40rpx;
	height:40rpx
}
.order{
	width: 90%;
	height: 230px;
	box-shadow:0px 2px 3px 0px rgba(222,222,222,1);
	margin-left: 5%;
	margin-top: 3%;
	background: white;
	border-radius: 10px;
}
.order-name{
	width: 100%;
	height: 400px;
	padding-left: 2%;
}
.orde{
	width: 100%;
	height: 50px;
	display: flex;
	padding-top: 3%;
}
.order-title{
	width: 7%;
}
.order-title image{
	width: 20px;
	height: 20px;
}
.orde-name{
	width: 73%;
	color: #323232;
	font-size: 15px;
	margin-top: 2px;
}
.order-ri{
	width: 20%;
	color: #EC6532;
	font-size: 15px;
	margin-top: 2px;
}
.order-con{
	width: 100%;
	height: 30px;
	display: flex;
	margin-top: 10px;
}
.order-con1{
	width: 100%;
	height: 20px;
