	margin-top: 10px;
}
.con-le{
	width: 80%;
	color: #323232;
	font-size: 14px;
	opacity: 1;
	padding-left: 2%;
}
.con-righ{
	width: 20%;
	color: #323232;
	font-size: 14px;
}
.con-left{
	width: 70%;
