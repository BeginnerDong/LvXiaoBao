	bottom: 10%;
	z-index: 2222;
	background: #FFEEF0;
}
.state-name{
	width: 100%;
	height: 90px;
}
.state-title{
	width: 100%;
	height: 30px;
	color: #323232;
	font-size: 16pt;
	line-height: 30px;
	padding-left: 3%;
	padding-top: 3%;
}
.state-btn{
	width: 100%;
	height: 40px;
	display: flex;
	margin-top: 10px;
}
.state-btn span{
	width: 80px;
	height: 40px;
	display: inline-block;
	text-align: center;
	line-height: 40px;
	border-radius: 10px;
	border: 1px solid #323232;
	color: #323232;
	margin-left: 5%;
	font-size: 14px;
	background: #C5C5C5;
}
.state-btn input{
	width: 80%;
	height: 30px;
	margin-left: 5%;
	border: 1px solid #E0E0E0;
	border-radius: 10px;
	
}
.state-btn input[placeholder]{
	padding-left: 3%;
