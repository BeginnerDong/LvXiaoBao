	font-size: 14px;
	opacity: 1;
	margin-top: 10px;
	padding-left: 2%;
}
.con-right{
	width: 30%;
	color: #323232;
	font-size: 14px;
	margin-top: 10px;
}
.con-right span{
	width: 43%;
	height: 20px;
	display: inline-block;
	border-radius: 30px;
	color: #323232;
	font-size: 11px;
	margin-right: 7%;
	text-align: center;
	line-height: 20px;
}

.conbtn{
	width: 100%;
	height: 40px;
	display: flex;
	margin-top: 15px;
}
.con-btn{
	width: 25%;
	font-size: 14px;
}
.con-btn span{
	width: 80px;
	height: 30px;
	border: 1px solid #323232;
	color: #323232;
	opacity: 0.6;
	text-align: center;
	line-height: 30px;
	font-size: 14px;
	border-radius: 20px;

	display: inline-block;
}
.bg{
	width: 100%;
	height: 100%;
	position: fixed;
	bottom: 0;
	background: #000000;
	opacity: 0.6;
	z-index: 111;
}
.state{
	width: 100%;
	height: 85%;
