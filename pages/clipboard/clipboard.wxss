/* pages/clipboard/clipboard.wxss */
page{
  background: #f5f5f5;
}
.textbox{ 
  padding: 40rpx;
  margin-bottom: 100rpx;
  background: #fff;
}
textarea{ 
  font-size:28rpx;
  line-height: 40rpx;
  color: #333; 
  width: 100%; 
  height: 400rpx; 
}
button{ 
  width: 80%;
  margin: 0 auto;
  height: 90rpx;
  font-size: 30rpx;
  font-weight: normal; 
  line-height: 90rpx; 
  border-radius: 50rpx; 
  text-align: center; 
  color: #fff;
  background:#ffb5a1;
  border: none;
  letter-spacing:20rpx;
}
button::after{
  border:none;
}

