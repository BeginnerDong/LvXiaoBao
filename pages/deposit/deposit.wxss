page{
	background: #f4f4f4;
}
.draw{
  padding: 0 3%;
	display: flex;
	background: white;
}
.draw-top{
	width: 90%;
	height: 100rpx;
	line-height: 100rpx;
	color: #323232;
	font-size: 30rpx;
	font-weight: 500;
}
.draw-imga{
	width: 10%;
	height: 100rpx;
	line-height: 100rpx;
  text-align: right;
}
.draw-imga image{
	width: 30rpx;
	height: 30rpx;
}
.drawa{
	padding: 0 3%;
	background: white;
	margin-top: 20rpx;
}
.draw-topa{
	height: 80rpx;
	line-height: 90rpx;
	border: 2rpx solid white;
	color: #323232;
	font-size: 32rpx;
	font-weight: bold;
	width: 100%;
	position: relative;
	z-index:1;
	border-bottom: 2rpx solid #E7E7E7;
  
}
.title-line {
	width: 32rpx;
	height: 6rpx;
	background: rgba(255, 209, 1, 1);
	border-radius: 4rpx;
	position:absolute;
	z-index:-1;
	left:5%;
	bottom: 14rpx;
}
.draw-cen{
	width: 100%;
	height: 100rpx;
	display: flex;
	border-bottom: 2rpx solid #E7E7E7;
}
.draw-ri{
	width: 30%;
	color: #5B669E;
	font-size: 22rpx;
	margin-top:30rpx;
	font-weight: 400;
	height: 40rpx;
	line-height: 40rpx;
}
.draw-img{
	width: 100%;
	display: flex;
	height: 100rpx;
	line-height:100rpx;
	border-bottom: 2rpx solid #E7E7E7;
}
.draw-con{
	width: 90%;
	color: #323232;
	font-size: 30rpx;
	font-weight: bold;
}
.drimg{
	width: 10%;
}
.drimg image{
	width:40rpx;
	height: 40rpx;
}
.cou-btn{
	width: 100%;
	margin: 80rpx 0;
	text-align: center;
}
.cou-btn span{
	width: 600rpx;
	height: 88rpx;
	text-align: center;
	color: #F4F3F3;
	background: #FF582B;
	line-height: 88rpx;
	display: inline-block;
	border-radius: 60rpx;
	font-weight: bold;
	font-size: 34rpx;
}
.couchar-inp{
	width: 100%;
	padding-left: 5%;
	line-height: 40rpx;
	color: #323232;
	font-size: 60rpx;
	font-weight: bold;
  display: flex;
  align-items: center;
}

.mnyInput{ 
  height: 80rpx;
  font-size: 44rpx;
  padding: 0 20rpx;
  box-sizing: border-box;
}
